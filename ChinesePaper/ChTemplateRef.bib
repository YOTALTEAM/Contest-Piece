@Article{Bhatt2013,
  author        = {Bhargav Bhatt and Peter Scholze},
  title         = {The pro-étale topology for schemes},
  year          = {2013},
  month         = sep,
  abstract      = {We give a new definition of the derived category of constructible $\ell$-adic sheaves on a scheme, which is as simple as the geometric intuition behind them. Moreover, we define a refined fundamental group of schemes, which is large enough to see all lisse $\ell$-adic sheaves, even on non-normal schemes. To accomplish these tasks, we define and study the pro-\'etale topology, which is a Grothendieck topology on schemes that is closely related to the \'etale topology, and yet better suited for infinite constructions typically encountered in $\ell$-adic cohomology. An essential foundational result is that this site is locally contractible in a well-defined sense.},
  archiveprefix = {arXiv},
  eprint        = {1309.1198},
  file          = {:http\://arxiv.org/pdf/1309.1198v2:PDF},
  keywords      = {math.AG, math.AC},
  primaryclass  = {math.AG},
}

@Article{Barwick2019,
  author        = {Clark Barwick and Peter Haine},
  title         = {Pyknotic objects, I. Basic notions},
  year          = {2019},
  month         = apr,
  abstract      = {Pyknotic objects are (hyper)sheaves on the site of compacta. These provide a convenient way to do algebra and homotopy theory with additional topological information present. This appears, for example, when trying to contemplate the derived category of a local field. In this article, we present the basic theory of pyknotic objects, with a view to describing a simple set of everyday examples.},
  archiveprefix = {arXiv},
  eprint        = {1904.09966},
  file          = {:http\://arxiv.org/pdf/1904.09966v2:PDF},
  keywords      = {math.AG, math.AT, math.CT},
  primaryclass  = {math.AG},
}

